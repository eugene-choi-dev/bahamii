        "heading_Tn9Gxf": {
          "type": "heading",
          "settings": {
            "heading": "What Are Bahamii Date & Energy Bars?",
            "heading_size": 30,
            "heading_size_mb": 30,
            "heading_line_height": 1,
            "heading_color": "#000"
          }
        },
        "text_8PeCUH": {
          "type": "text",
          "settings": {
            "text": "<div align=\"left\"><p>Bahamii Date & Nut Energy Bars are organic and combine superfood ingredients to provide a nutrient dense, convenient, and tasty energy bars for diabetics, athletes, and even kids. Enjoy them with your morning coffee, before a workout, on a hike or on any other adventure!<\/p><\/div>",
            "text_size": 16,
            "text_size_mb": 12,
            "text_line_height": 1,
            "margin_top_text": 20,
            "text_color": "#000",
            "text_width": "800px"
          }
        }
      },
